#!/bin/bash
#
# Gets status of all modules being used for current development

. $HOME/.tixrc

pushd $MODULE_ROOT >/dev/null
    for module in */; do
        module_name=$($module | tr -d '/')
        if [ -d "$module/.git" ]; then
            status-module $module_name 2>&1
        fi
    done
popd >/dev/null
