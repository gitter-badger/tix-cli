#!/bin/bash
#
# Git commits and NPM publishes gyp and node-gyp modules

source $HOME/.tixrc

pushd $NET_ROOT
    echo "--Restoring packages--"
    nuget restore Tix.SelfHost.sln -PackagesDirectory packages
    echo "--Packages restored--"

    echo "--Building artifacts--"
    cmd //c msbuild Tix.Web.SelfHost\\Tix.Web.SelfHost.csproj //p:Configuration=Debug //p:OutputPath=..\\..\\artifacts\\tixinc-net
    echo "--Artifacts built--"
popd
