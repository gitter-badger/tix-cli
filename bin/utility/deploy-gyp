#!/bin/bash
#
# Git commits and NPM publishes gyp and node-gyp modules

source $HOME/.tixrc

echo "--publishing gyp--"
publish-gyp

echo "--installing node-gyp--"
install-node-gyp

pushd $CLI_ROOT
    echo "--removing @tixinc/automation--"
    npm remove @tixinc/automation
    echo "--installing @tixinc/automation--"
    npm install @tixinc/automation &>$GYP_DEPLOY_LOG
popd